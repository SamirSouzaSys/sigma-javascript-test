    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "strict": true,
        "outDir": "./dist",
        "moduleResolution": "node",
        "paths": {
            "*": ["./node_modules/*"]
        }
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts"
    ]
}